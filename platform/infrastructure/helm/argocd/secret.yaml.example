# JFrog Helm Repository Credentials for ArgoCD
#
# Usage:
#   1. Copy this file: cp secret.yaml.example secret.yaml
#   2. Replace YOUR_JFROG_TOKEN_HERE with actual token
#   3. Apply: kubectl apply -f secret.yaml
#
# Note: Never commit secret.yaml to git!
apiVersion: v1
kind: Secret
metadata:
  name: jfrog-helm-repo
  namespace: argocd
  labels:
    argocd.argoproj.io/secret-type: repository
type: Opaque
stringData:
  type: helm
  name: jfrog-helm
  url: https://your-company.jfrog.io/artifactory/helm-local
  username: ""
  password: "YOUR_JFROG_TOKEN_HERE"
