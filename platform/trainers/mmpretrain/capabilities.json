  "task_types": [
    {
      "name": "classification",
      "display_name": "Image Classification",
      "description": "Classify images into predefined categories",
      "supported": true
    }
  ],
  "dataset_formats": [
    {
      "name": "imagefolder",
      "display_name": "ImageFolder Format",
      "description": "Folder structure where each subfolder is a class (PyTorch default)",
      "supported": true
    },
    {
      "name": "dice",
      "display_name": "DICE Format",
      "description": "Platform's internal DICE format (auto-converted to ImageFolder)",
      "supported": true
    }
  ],
  "metric_candidates_by_task": {
    "classification": [
      {
        "value": "accuracy_top1",
        "label": "Top-1 Accuracy",
        "mode": "max",
        "description": "정확도 (가장 높은 예측이 정답과 일치)"
      },
      {
        "value": "accuracy_top5",
        "label": "Top-5 Accuracy",
        "mode": "max",
        "description": "상위 5개 예측 중 정답 포함 비율"
      },
      {
        "value": "precision",
        "label": "Precision (정밀도)",
        "mode": "max",
        "description": "예측한 양성 중 실제 양성 비율"
      },
      {
        "value": "recall",
        "label": "Recall (재현율)",
        "mode": "max",
        "description": "실제 양성 중 올바르게 예측한 비율"
      },
      {
        "value": "f1_score",
        "label": "F1 Score",
        "mode": "max",
        "description": "Precision과 Recall의 조화 평균"
      }
    ]
  },
  "config_schema": {
    "epochs": {
      "type": "int",
      "default": 100,
      "min": 1,
      "max": 500,
      "description": "Number of training epochs"
    },
    "batch_size": {
      "type": "int",
      "default": 32,
      "min": 1,
      "max": 256,
      "description": "Batch size per GPU"
    },
    "learning_rate": {
      "type": "float",
      "default": 0.1,
      "min": 0.00001,
      "max": 1.0,
      "description": "Base learning rate"
    },
    "img_size": {
      "type": "int",
      "default": 224,
      "options": [224, 256, 384, 512],
      "description": "Input image size"
    },
    "optimizer": {
      "type": "select",
      "default": "SGD",
      "options": ["SGD", "AdamW", "Adam", "LAMB"],
      "description": "Optimizer algorithm"
    },
    "lr_scheduler": {
      "type": "select",
      "default": "cosine",
      "options": ["step", "cosine", "linear", "exp"],
      "description": "Learning rate scheduler"
    },
    "weight_decay": {
      "type": "float",
      "default": 0.0001,
      "min": 0.0,
      "max": 0.1,
      "description": "Weight decay for regularization"
    },
    "warmup_epochs": {
      "type": "int",
      "default": 5,
      "min": 0,
      "max": 50,
      "description": "Number of warmup epochs"
    },
    "pretrained": {
      "type": "bool",
      "default": true,
      "description": "Use pretrained weights from ImageNet"
    },
    "mixup_alpha": {
      "type": "float",
      "default": 0.0,
      "min": 0.0,
      "max": 1.0,
      "description": "Mixup augmentation alpha (0 = disabled)"
    },
    "cutmix_alpha": {
      "type": "float",
      "default": 0.0,
      "min": 0.0,
      "max": 1.0,
      "description": "CutMix augmentation alpha (0 = disabled)"
    },
    "label_smoothing": {
      "type": "float",
      "default": 0.0,
      "min": 0.0,
      "max": 0.5,
      "description": "Label smoothing factor"
    }
  },
  "export_formats": ["onnx", "torchscript", "tensorrt", "coreml"],
  "requirements": {
    "python": ">=3.8,<3.12",
    "pytorch": ">=1.8.0",
    "cuda": ">=11.0",
    "mmcv": ">=2.0.0",
    "mmengine": ">=0.7.1",
    "mmpretrain": ">=1.0.0"
  }
}
