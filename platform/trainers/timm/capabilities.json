{
  "framework": "timm",
  "version": "0.9.12",
  "description": "PyTorch Image Models - Image Classification",
  "supported_tasks": ["image_classification"],
  "models": {
    "resnet18": {
      "display_name": "ResNet-18",
      "task_type": "image_classification",
      "description": "18-layer ResNet for image classification",
      "default_config": {
        "epochs": 100,
        "batch_size": 32,
        "learning_rate": 0.001,
        "imgsz": 224
      }
    },
    "resnet34": {
