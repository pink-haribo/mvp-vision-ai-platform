        "batch_size": 32,
        "learning_rate": 0.001,
        "imgsz": 224
      }
    }
  },
  "export_formats": ["onnx", "torchscript"],
  "config_schema": {
    "epochs": {
      "type": "int",
      "default": 100,
      "min": 1,
      "max": 1000,
      "description": "Number of training epochs"
    },
    "batch_size": {
      "type": "int",
      "default": 32,
      "min": 1,
      "max": 512,
      "description": "Batch size for training"
    },
    "learning_rate": {
      "type": "float",
      "default": 0.001,
      "min": 0.000001,
      "max": 1.0,
      "description": "Initial learning rate"
    },
    "imgsz": {
      "type": "int",
      "default": 224,
      "options": [224, 240, 260, 300, 320, 384, 448, 512],
      "description": "Input image size"
    },
    "optimizer": {
      "type": "select",
      "default": "adamw",
      "options": ["adam", "adamw", "sgd"],
      "description": "Optimizer algorithm"
    },
    "scheduler": {
      "type": "select",
      "default": "cosine",
      "options": ["cosine", "step", "plateau", "none"],
      "description": "Learning rate scheduler"
    },
    "weight_decay": {
      "type": "float",
      "default": 0.0001,
      "min": 0.0,
      "max": 0.1,
      "description": "Weight decay for regularization"
    },
    "pretrained": {
      "type": "bool",
      "default": true,
      "description": "Use pretrained weights"
    }
  }
}
