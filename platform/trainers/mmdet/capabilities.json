      "supported": true
    },
    {
      "name": "dice",
      "display_name": "DICE Format",
      "description": "Platform's internal DICE format (auto-converted to COCO)",
      "supported": true
    }
  ],
  "metric_candidates_by_task": {
    "detection": [
      {
        "value": "bbox_mAP",
        "label": "mAP (평균 정밀도)",
