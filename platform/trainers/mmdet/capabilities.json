      }
    ]
  },
  "config_schema": {
    "epochs": {
      "type": "int",
      "default": 12,
      "min": 1,
      "max": 300,
      "description": "Number of training epochs (default 12 for 1x schedule)"
    },
    "batch_size": {
      "type": "int",
      "default": 2,
      "min": 1,
      "max": 32,
      "description": "Batch size per GPU (MMDet uses smaller batches)"
    },
    "learning_rate": {
      "type": "float",
      "default": 0.02,
      "min": 0.0001,
      "max": 0.1,
      "description": "Base learning rate (scales with batch size)"
    },
    "optimizer": {
      "type": "select",
      "default": "SGD",
      "options": ["SGD", "AdamW"],
      "description": "Optimizer algorithm"
    },
    "lr_scheduler": {
      "type": "select",
      "default": "step",
      "options": ["step", "cosine", "linear"],
      "description": "Learning rate scheduler"
    },
    "warmup_iters": {
      "type": "int",
      "default": 500,
      "min": 0,
      "max": 5000,
      "description": "Number of warmup iterations"
    },
    "img_scale": {
      "type": "tuple",
      "default": [1333, 800],
      "description": "Training image scale (max_size, min_size)"
    },
    "pretrained": {
      "type": "bool",
      "default": true,
      "description": "Use pretrained backbone weights"
    },
    "load_from": {
      "type": "string",
      "default": null,
      "description": "Path or URL to pretrained model checkpoint"
    },
    "resume_from": {
      "type": "string",
      "default": null,
      "description": "Path to checkpoint to resume training from"
    },
    "val_interval": {
      "type": "int",
      "default": 1,
      "min": 1,
      "max": 50,
      "description": "Validation interval (epochs)"
    }
  },
  "export_formats": ["onnx", "torchscript", "tensorrt"],
  "requirements": {
    "python": ">=3.8,<3.12",
    "pytorch": ">=1.8.0",
    "cuda": ">=11.0",
    "mmcv": ">=2.0.0",
    "mmengine": ">=0.7.1",
    "mmdet": ">=3.0.0"
  }
