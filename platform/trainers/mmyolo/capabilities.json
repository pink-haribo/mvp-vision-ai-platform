  "config_schema": {
    "epochs": {
      "type": "int",
      "default": 300,
      "min": 1,
      "max": 1000,
      "description": "Number of training epochs (YOLO typically uses 300)"
    },
    "batch_size": {
      "type": "int",
      "default": 16,
      "min": 1,
      "max": 128,
      "description": "Batch size per GPU"
    },
    "learning_rate": {
      "type": "float",
      "default": 0.01,
      "min": 0.0001,
      "max": 0.1,
      "description": "Base learning rate"
    },
    "img_scale": {
      "type": "tuple",
      "default": [640, 640],
      "description": "Training image scale (height, width)"
    },
    "optimizer": {
      "type": "select",
      "default": "SGD",
      "options": ["SGD", "AdamW"],
      "description": "Optimizer algorithm"
    },
    "warmup_epochs": {
      "type": "int",
      "default": 3,
      "min": 0,
      "max": 30,
      "description": "Number of warmup epochs"
    },
    "weight_decay": {
      "type": "float",
      "default": 0.0005,
      "min": 0.0,
      "max": 0.1,
      "description": "Weight decay for regularization"
    },
    "pretrained": {
      "type": "bool",
      "default": true,
      "description": "Use pretrained weights"
    },
    "val_interval": {
      "type": "int",
      "default": 10,
      "min": 1,
      "max": 100,
      "description": "Validation interval (epochs)"
    },
    "mosaic": {
      "type": "bool",
      "default": true,
      "description": "Enable mosaic augmentation"
    },
    "mixup": {
      "type": "bool",
      "default": true,
      "description": "Enable mixup augmentation"
    },
    "close_mosaic_epochs": {
      "type": "int",
      "default": 10,
      "min": 0,
      "max": 50,
      "description": "Disable mosaic for last N epochs"
    }
  },
  "export_formats": ["onnx", "torchscript", "tensorrt", "ncnn"],
  "requirements": {
    "python": ">=3.8,<3.12",
